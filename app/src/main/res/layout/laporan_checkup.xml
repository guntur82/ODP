<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <LinearLayout
        android:layout_width="124dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/_234sdp"
        android:layout_marginTop="@dimen/_330sdp">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fab3"
            android:layout_width="@dimen/_50sdp"
            android:layout_height="@dimen/_50sdp"
            android:layout_gravity="bottom|end"
            android:layout_margin="@dimen/_15sdp"
            app:srcCompat="@android:drawable/ic_btn_speak_now" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="77dp"
        android:layout_marginLeft="@dimen/_235sdp"
        android:layout_marginTop="@dimen/_400sdp">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fab2"
            android:layout_width="58dp"
            android:layout_height="77dp"
            android:layout_gravity="bottom|end"
            android:layout_margin="@dimen/_15sdp"
            app:srcCompat="@android:drawable/ic_menu_camera" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_450sdp"
        android:layout_marginLeft="@dimen/_250sdp">
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab1"
        android:layout_width="58dp"
        android:layout_height="77dp"
        android:layout_gravity="bottom|end"
        app:srcCompat="@android:drawable/ic_dialog_map" />
    </LinearLayout>
<LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/_500sdp"
    android:layout_marginLeft="@dimen/_250sdp">
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        app:srcCompat="@android:drawable/ic_dialog_email" />
</LinearLayout>
</RelativeLayout>